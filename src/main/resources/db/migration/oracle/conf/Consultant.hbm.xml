<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="common.Consultant" table="consultant" catalog="oifyi">
        <id name="id" type="int">
            <column name="ID" not-null="true" unique="true"/>
            <generator class="increment"/>
        </id>
        <property name="nom" type="string" column="NOM" length="30" not-null="true" unique="false"/>
        <property name="prenom" type="string" column="PRENOM" length="30" not-null="true" unique="false"/>
        <property name="mail" type="string" column="MAIL" not-null="false" length="30" unique="false"/>
    </class>
</hibernate-mapping>