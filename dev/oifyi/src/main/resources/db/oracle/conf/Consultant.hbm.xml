<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="main.java.common.Consultant" table="consultant" catalog="oifyi">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="nom" type="java.lang.String">
            <column name="nom" length="30" not-null="true" unique="false"/>
        </property>
        <property name="prenom" type="java.lang.String">
            <column name="prenom" length="30" not-null="true" unique="false"/>
        </property>
    </class>
</hibernate-mapping>